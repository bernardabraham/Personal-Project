package test2;

public class Factorial {

	// public int factorialBA(int input) {

	public int factorialAK(int x) {
		int factorial = -1;

		for (int i = 2; x % i == 0; i++) {

			if (factorial == 1) {
				factorial = i;
				break;
			}

			factorial = x / i;
		}

		return factorial;
	}

}

