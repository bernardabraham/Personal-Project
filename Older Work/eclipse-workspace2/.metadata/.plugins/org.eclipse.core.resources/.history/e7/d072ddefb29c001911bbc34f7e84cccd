package qa.com.TddLibrary2;

public abstract class Item {
	
	private int pages;
	private double cost;
	
	public Item(int pages, int cost) {
		super();
		this.pages = pages;
		this.cost = cost;
	}

	public int getPages() {
		return pages;
	}

	public void setPages(int pages) {
		this.pages = pages;
	}

	public int getCost() {
		return cost;
	}

	public void setCost(int cost) {
		this.cost = cost;
	}
	
	
	
}
